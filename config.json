{
    "locals": {
        "title": "glacierresearch.org",
        "development": true
    },
    "require": {
        "moment": "moment",
        "typogr": "typogr",
        "url": "url",
        "_": "underscore"
    },
    "plugins": [
        "./plugins/paginator.coffee",
        "wintersmith-data-json",
        "wintersmith-less",
        "wintersmith-browserify"
        ],
    "markdown": {
        "smartLists": true,
        "smartypants": true
    },
    "paginator": {
        "perPage": 5,
        "template": "blogIndex.jade",
        "articles": "blog/posts",
        "first": "blog/index.html",
        "filename": "blog/page/%d/index.html"
    },
    "port": 8888,
    "ignore": [".*"]
}
